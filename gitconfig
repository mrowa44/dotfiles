[alias]
  aa = add --all
  ap = add --patch
  br = branch
  co = checkout
  cm = commit -v
  df = diff
  st = status
  lg = log --abbrev-commit --date=relative --graph --pretty=format:'%C(yellow)%h%Creset %s %Cblue~ %cn %Creset(%Cgreen%ar)%Cred%d%Creset'
  lo = log --graph --decorate --oneline --color
  last = log -1 HEAD
[grep]
 lineNumber = true
[push]
