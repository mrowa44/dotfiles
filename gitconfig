[user]
	name = Justyna Rachowicz
	email = mrowa44@gmail.com
[grep]
  lineNumber = true
[push]
  default = current
[color]
  ui     = auto
  status = auto
  branch = auto
  diff   = auto
[log]
  decorate = true
[help]
  autocorrect = 1
[core]
  excludesfile = ~/.gitignore
  ignorecase = true
[pager]
  diff = diff-so-fancy | less --tabs=1,5 -RFX
  show = diff-so-fancy | less --tabs=1,5 -RFX
[rerere]
  enabled = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[alias]
  unused = !git branch --merged main | grep -v '^[ *]*main$' | xargs git branch -d
  unused-prod = !git branch --merged prod | grep -v '^[ *]*prod$' | xargs git branch -d
  recent = !git branch --sort=-committerdate | HEAD -10
[credential]
	helper = osxkeychain
[commit]
	gpgsign = false
[pull]
	rebase = true
[init]
	defaultBranch = main

