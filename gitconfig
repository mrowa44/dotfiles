[grep]
  lineNumber = true
[push]
  default = current
[color]
  ui     = auto
  status = auto
  branch = auto
  diff   = auto
[log]
  decorate = true
[help]
  autocorrect = 1
[core]
  excludesfile = ~/.gitignore
[pager]
  diff = diff-so-fancy | less --tabs=1,5 -RFX
  show = diff-so-fancy | less --tabs=1,5 -RFX
[rerere]
  enabled = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[alias]
  unused = !git branch --merged master | ag -L master | xargs -n 1 git branch -d
[credential]
	helper = osxkeychain
